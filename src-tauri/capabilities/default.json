{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "npm-run",
          "cmd": "npm",
          "args": true,
          "sidecar": false
        }
      ]
    },
    "fs:default",
    "fs:allow-app-write",
    "fs:allow-read",
    "fs:read-all",
    "fs:allow-read-file",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "core:app:allow-app-hide",
    "core:menu:default",
    "process:default",
    "dialog:default"
  ]
}